{
  "name": "firefox-test-tube",
  "private": true,
  "proxy": "http://localhost:8000",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "react-scripts build",
    "build:css": "stylus --sourcemap ./src/components/views/styl/* --out ./src/components/views/css/",
    "start": "npm-run-all --parallel start:js watch:css",
    "start:js": "react-scripts start",
    "watch:css": "stylus --watch --sourcemap src/components/views/styl/* --out ./src/components/views/css/",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "chart.js": "2.7.2",
    "dateformat": "3.0.3",
    "distinct-colors": "1.0.4",
    "gravatar": "1.6.0",
    "npm-run-all": "4.1.3",
    "query-string": "5.1.1",
    "react": "16.4.0",
    "react-chartjs-2": "2.7.2",
    "react-dom": "16.4.0",
    "react-fa": "5.0.0",
    "react-paginate": "5.2.3",
    "react-refetch": "1.0.4",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.4",
    "sifter": "0.5.3",
    "stylus": "0.54.5"
  }
}
