{
  "name": "firefox-test-tube",
  "private": true,
  "proxy": "http://localhost:8000",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "react-scripts build",
    "build:css": "stylus --sourcemap ./src/components/views/styl/* --out ./src/components/views/css/",
    "start": "npm-run-all --parallel start:js watch:css",
    "start:js": "react-scripts start",
    "watch:css": "stylus --watch --sourcemap src/components/views/styl/* --out ./src/components/views/css/",
    "eject": "react-scripts eject",
    "posttest": "npm audit",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "chart.js": "2.7.2",
    "dateformat": "3.0.3",
    "distinct-colors": "1.0.4",
    "gravatar": "1.6.0",
    "npm-run-all": "4.1.5",
    "plotly.js": "1.43.1",
    "query-string": "5.1.1",
    "react": "16.5.2",
    "react-chartjs-2": "2.7.4",
    "react-dom": "16.5.2",
    "react-fa": "5.0.0",
    "react-paginate": "5.2.4",
    "react-plotly.js": "2.2.0",
    "react-refetch": "2.0.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.5",
    "sifter": "0.5.3",
    "stylus": "0.54.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
